// compiler test for checking if the
// concatenation machinery works with
// contiguous-allocated arrays

pub fn main() void = io & conv {
    new here | 128 {
        let foo = new ! [1, 2, 3] u32 @ here;
        let bar = new ! [4, 5, 6] u32 @ here;
        let egg = cat foo ..! bar @ here;
        for itm, idx .. egg {
            test itm u64 == idx + 1;
        };
    }!;
};
