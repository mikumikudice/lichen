// parsing test for recursive function call

io = use "io.lim";

pub fn main() void = io {
    let x = factorial(4);
    if x == 24 {
        io::print("worked!\n");
    } else {
        io::print("failed :c\n");
    };
};

fn factorial(n u64) u64 = {
    => if n > 1 {
        => n * factorial(n - 1);
    } else {
        => n;
    };
};
