// parser test for asserting for
// correct behavior of borrowing

fn borrow(obj str) str @ obj = {
    return obj;
};

pub fn main() void = {
    new outer | 128 {
        let mut obj = new ! "test ok" @ outer;
        new inner | 128 {
            let bar = new ! obj @ inner;
            obj = borrow(bar);
        }!;
    }!;
};
