io mod = use "std/io.lim";

fn map(data [8]str, predicate fn(str) u64 #any) [8]u64 = #any {
    let mut data' = [8; 0...] u64;
    for mut itm, idx .. data' {
        itm = predicate(data[idx]);
    };
    return data';
};

fn pre(src str) u64 = {
    return #src;
};

pub fn main() void = io {
    map(["mia", "leo", "lue"], pre);
};
