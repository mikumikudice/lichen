// parsing test for if-else blocks,
// comparisons, lazy evaluation and
// type casting

io mod = use "std/io.lim";

pub fn main() void = io {
    let x = 6 u64;
    let y = 7 u32;
    let z = x * y u64;

    if x < y u64 {
        io::println("test 0 ok")!;
    } else {
        io::println("test 0 fail")!;
    };

    if x > 0 || y > 0 {
        io::println("test 1 ok")!;
    } else {
        io::println("test 1 fail")!;
    };

    if x + 1 == y u64 {
        io::println("test 2 ok")!;
    } else {
        io::println("test 2 fail")!;
    };

    if z > 0 && x u32 == y - 1 {
        io::println("test 3 ok")!;
    } else {
        io::println("test 3 fail")!;
    };

    if z > 0 && z == x * y u64 {
        io::println("test 4 ok")!;
    } else {
        io::println("test 4 fail")!;
    };

    if (z - z) > 0 || x > 0 {
        io::println("test 5 ok")!;
    } else {
        io::println("test 5 fail")!;
    };

    if x u32 == y || x < z {
        io::println("test 6 ok")!;
    } else {
        io::println("test 6 fail")!;
    };
};
